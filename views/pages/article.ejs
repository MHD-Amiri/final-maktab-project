<!DOCTYPE html>
<html>

<%- include('../partials/header.ejs') %>
<title><%= title %></title>

<body>
    <div class="container">
        <div class="jumbotron">
            <div class="form-group">
                <label for="title">Title</label>
                <input type="text" class="form-control" id="title" placeholder="title" name="title">
            </div>
            <div class="form-group">
                <label for="bio">Content</label>
                <textarea class="form-control" id="content" placeholder="Write your article content here" rows="17"
                    name="content"></textarea>
                <small id="emailHelp" class="form-text">Feel free to resize the textarea for your own desired size. Just
                    grab the right down corner and pull</small>
            </div>
            <p class="lead">
                <a class="btn btn-primary btn-lg" href="#" role="button">Save Article</a>
            </p>
        </div>
    </div>

    <%- include('../partials/scripts.ejs') %>
</body>

</html>